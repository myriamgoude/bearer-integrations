{
  "authorizationURL": "https://app.hubspot.com/oauth/authorize",
  "tokenURL": "https://api.hubapi.com/oauth/v1/token",
  "authType": "OAUTH2",
  "tokenParams": {
    "redirect_uri": "{{CALLBACK_URL_V2}}",
    "grant_type": "authorization_code"
  },
  "authorizationParams": {},
  "config": {
    "callbackURL": "{{CALLBACK_URL_V2}}",
    "response_type": "code",
    "scope": ["contacts"]
  },
  "setupViews": [
    {
      "type": "text",
      "label": "Client ID",
      "controlName": "clientID"
    },
    {
      "type": "password",
      "label": "Client Secret",
      "controlName": "clientSecret"
    }
  ]
}
