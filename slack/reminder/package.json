{
  "name": "slack-reminder",
  "version": "0.0.1",
  "description": "SlackReminder",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "module": "dist/esm/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "scripts": {
    "start": "bearer start",
    "test": "jest --no-cache",
    "push": "bearer push",
    "invoke": "bearer invoke"
  },
  "dependencies": {
    "@bearer/core": "beta5",
    "@bearer/intents": "beta5",
    "@bearer/ui": "beta5",
    "axios": "^0.18.0",
    "ionicons": "^4.5.5"
  },
  "devDependencies": {
    "@bearer/cli": "beta5",
    "@types/jest": "latest",
    "@types/node": "latest",
    "jest": "latest"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}